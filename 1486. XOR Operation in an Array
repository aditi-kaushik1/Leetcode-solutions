class Solution {
    public int xorOperation(int n, int start) {
        /*Solution 1*/
        // int[] nums=new int[n];
        // int xor_op=0;
        // for(int i=0;i<n;i++){
        //     nums[i]=start+2*i;
        //     xor_op^=nums[i];
        // }
        // return xor_op;
        
        /*Solution 2*/
        int result=0;
        for(int i=0;i<n;i++){
            result^=(start+2*i);
        }
        return result;
    }
}
